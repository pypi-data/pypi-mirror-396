<?xml version="1.0" ?>
<datasource formatted_name="federated.odgs" inline="true">
   <connection class_="federated">
      <named-connections>
         <named-connection caption="ODGS_Source" name="federated.odgs">
            <connection class_="textscan"/>
         </named-connection>
      </named-connections>
   </connection>
   <column caption="Net Profit Margin" datatype="real" name="[Net Profit Margin]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(net_income)/SUM(revenue))*100"/>
      <desc>
         <formatted-text>
            <run>Represents the percentage of revenue that remains after all operating expenses, interest, and taxes have been deducted.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Return on Invested Capital (ROIC)" datatype="real" name="[Return on Invested Capital (ROIC)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(net_operating_profit_after_tax))/(SUM(total_debt)+SUM(total_equity))"/>
      <desc>
         <formatted-text>
            <run>Measures how well a company is using its money to generate returns.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Free Cash Flow (FCF)" datatype="real" name="[Free Cash Flow (FCF)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(operating_cash_flow)-SUM(capital_expenditures)"/>
      <desc>
         <formatted-text>
            <run>The cash a company produces through its operations after subtracting capital expenditures.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="EBITDA Margin" datatype="real" name="[EBITDA Margin]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(ebitda)/SUM(revenue))*100"/>
      <desc>
         <formatted-text>
            <run>Earnings before interest, taxes, depreciation, and amortization as a percentage of revenue.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Gross Profit Margin" datatype="real" name="[Gross Profit Margin]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="((SUM(revenue)-SUM(cost_of_goods_sold))/SUM(revenue))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of revenue remaining after direct costs of goods sold are deducted.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Working Capital Ratio" datatype="real" name="[Working Capital Ratio]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(current_assets)/SUM(current_liabilities)"/>
      <desc>
         <formatted-text>
            <run>Measures a company's ability to meet short-term obligations with current assets.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Employee Turnover Rate" datatype="real" name="[Employee Turnover Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_employees_who_left)/SUM(average_number_of_employees))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of employees who leave a company within a certain period.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Lost Time Injury Frequency Rate (LTIFR)" datatype="real" name="[Lost Time Injury Frequency Rate (LTIFR)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_lost)-SUM(time_injuries)/SUM(total_hours_worked))*SUM(1,000,000)"/>
      <desc>
         <formatted-text>
            <run>The number of lost-time injuries occurring in a workplace per 1,000,000 hours worked.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Training Hours Per Employee" datatype="real" name="[Training Hours Per Employee]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_training_hours)/SUM(number_of_employees)"/>
      <desc>
         <formatted-text>
            <run>Average number of hours spent on training and development per employee per year.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Absenteeism Rate" datatype="real" name="[Absenteeism Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(total_days_absent)/SUM(total_available_work_days))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of scheduled work time lost due to employee absences.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="IT System Uptime" datatype="real" name="[IT System Uptime]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="((SUM(total_time)-SUM(downtime))/SUM(total_time))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of time a critical IT system (e.g., ERP, ERP, SCADA) is operational and available.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Mean Time To Repair (MTTR)" datatype="real" name="[Mean Time To Repair (MTTR)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_repair_time)/SUM(number_of_repairs)"/>
      <desc>
         <formatted-text>
            <run>Average time required to repair a failed system or component and restore it to operational status.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Cybersecurity Incident Rate" datatype="real" name="[Cybersecurity Incident Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(number_of_security_incidents)/SUM(time_period)"/>
      <desc>
         <formatted-text>
            <run>The number of confirmed security incidents per period.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Warehouse Dwell Time (Import)" datatype="real" name="[Warehouse Dwell Time (Import)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(average)(SUM(gate)-SUM(out_timestamp)-SUM(discharge_timestamp))"/>
      <desc>
         <formatted-text>
            <run>The average time an import container resides on a marine terminal, from vessel discharge to truck gate-out.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Dock Moves Per Hour (BMPH)" datatype="real" name="[Dock Moves Per Hour (BMPH)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_package_moves)/(SUM(transport_unit_departure_time)-SUM(transport_unit_arrival_time))"/>
      <desc>
         <formatted-text>
            <run>The total number of container moves (lifts on/off a vessel) divided by the number of hours the vessel is at berth.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Truck Turnaround Time" datatype="real" name="[Truck Turnaround Time]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(average)(SUM(gate)-SUM(out_timestamp)-SUM(gate)-SUM(in_timestamp))"/>
      <desc>
         <formatted-text>
            <run>The average time it takes for a truck to enter the terminal, complete its transaction (pickup/drop-off), and exit.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="On-Time Performance (Transport Unit/Flight/Truck)" datatype="real" name="[On-Time Performance (Transport Unit/Flight/Truck)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_on)-SUM(time_events)/SUM(total_number_of_events))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of arrivals or departures that occur within a predefined time window of the schedule.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Cargo Damage Rate" datatype="real" name="[Cargo Damage Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_damaged_units)/SUM(total_units_handled))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of cargo units that sustain damage during handling or storage.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Transport Unit Turnaround Time" datatype="real" name="[Transport Unit Turnaround Time]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(transport_unit_departure_time)-SUM(dock_arrival_time)"/>
      <desc>
         <formatted-text>
            <run>Total time from when a vessel arrives at berth to when it departs.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Fleet Utilization Rate" datatype="real" name="[Fleet Utilization Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(total_operating_hours)/SUM(total_available_hours))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of time vehicles in a fleet are actively being used for revenue-generating activities.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Fuel Efficiency (MPG or L/100km)" datatype="real" name="[Fuel Efficiency (MPG or L/100km)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_distance_traveled)/SUM(total_fuel_consumed)"/>
      <desc>
         <formatted-text>
            <run>Average fuel consumption efficiency across fleet operations.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Overall Equipment Effectiveness (OEE)" datatype="real" name="[Overall Equipment Effectiveness (OEE)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(availability_rate)*SUM(performance_rate)*SUM(quality_rate)"/>
      <desc>
         <formatted-text>
            <run>A composite metric that measures manufacturing productivity by combining equipment availability, performance, and quality.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Manufacturing Cycle Time" datatype="real" name="[Manufacturing Cycle Time]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(process_time)+SUM(move_time)+SUM(inspection_time)+SUM(queue_time)"/>
      <desc>
         <formatted-text>
            <run>The total time required to convert raw materials into finished goods.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="First Pass Yield (FPY)" datatype="real" name="[First Pass Yield (FPY)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(units_completed_without_rework)/SUM(total_units_started))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of products manufactured correctly to specifications the first time, without rework or scrap.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Scrap Rate" datatype="real" name="[Scrap Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(weight_of_scrap)/SUM(total_weight_of_materials_used))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of raw materials or work-in-process that becomes waste during production.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Production Capacity Utilization" datatype="real" name="[Production Capacity Utilization]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(actual_output)/SUM(maximum_possible_output))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of maximum production capacity currently being utilized.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Downtime Percentage" datatype="real" name="[Downtime Percentage]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(downtime_hours)/SUM(scheduled_operating_hours))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of scheduled production time lost due to equipment failures or maintenance.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Changeover Time" datatype="real" name="[Changeover Time]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(end_of_last_good_part)(SUM(product_a))SUM(to_first_good_part)(SUM(product_b))"/>
      <desc>
         <formatted-text>
            <run>Time required to switch production line from one product to another.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Maintenance Cost as % of Asset Value" datatype="real" name="[Maintenance Cost as % of Asset Value]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(annual_maintenance_costs)/SUM(total_asset_replacement_value))*100"/>
      <desc>
         <formatted-text>
            <run>Annual maintenance and repair costs expressed as percentage of total asset replacement value.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Asset Uptime" datatype="real" name="[Asset Uptime]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="((SUM(scheduled_operating_time)-SUM(unscheduled_downtime))/SUM(scheduled_operating_time))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of time a critical piece of equipment is available and running as intended.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Production Volume (Barrels/Day, MWh)" datatype="real" name="[Production Volume (Barrels/Day, MWh)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(sum_of_production_over_period)"/>
      <desc>
         <formatted-text>
            <run>The total amount of product produced over a specific period.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Mean Time Between Failures (MTBF)" datatype="real" name="[Mean Time Between Failures (MTBF)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_operational_time)/SUM(number_of_failures)"/>
      <desc>
         <formatted-text>
            <run>The predicted elapsed time between inherent failures of a mechanical or electronic system during normal operation.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Energy Intensity" datatype="real" name="[Energy Intensity]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_energy_consumed)(SUM(kwh))/SUM(total_units_produced)"/>
      <desc>
         <formatted-text>
            <run>Energy consumed per unit of production output.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Generation Reliability" datatype="real" name="[Generation Reliability]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="((SUM(available_hours)-SUM(forced_outage_hours))/SUM(available_hours))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of time a power generation asset is available to produce when needed.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Emissions Rate (CO2/Unit)" datatype="real" name="[Emissions Rate (CO2/Unit)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_co2_emissions)(SUM(tons))/SUM(total_production)(SUM(units))"/>
      <desc>
         <formatted-text>
            <run>Amount of greenhouse gas emissions per unit of production.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Water Usage Efficiency" datatype="real" name="[Water Usage Efficiency]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_water_consumed)(SUM(gallons))/SUM(total_units_produced)"/>
      <desc>
         <formatted-text>
            <run>Volume of water consumed per unit of production output.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Inventory Turnover" datatype="real" name="[Inventory Turnover]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(cost_of_goods_sold)/SUM(average_inventory)"/>
      <desc>
         <formatted-text>
            <run>Measures how many times a company's inventory is sold and replaced over a period.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Perfect Order Rate" datatype="real" name="[Perfect Order Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_perfect_orders)/SUM(total_orders))*100"/>
      <desc>
         <formatted-text>
            <run>The percentage of orders delivered to the right place, with the right product, at the right time, in the right condition, with the right paperwork.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Days of Supply (DOS)" datatype="real" name="[Days of Supply (DOS)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(average_inventory)/(SUM(cost_of_goods_sold)/SUM(number_of_days_in_period))"/>
      <desc>
         <formatted-text>
            <run>The number of days a company could operate on its current inventory.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Order Fill Rate" datatype="real" name="[Order Fill Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(orders_filled_completely)/SUM(total_orders))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of customer orders fulfilled completely from available inventory.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Stockout Rate" datatype="real" name="[Stockout Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(days_out_of_stock)/SUM(total_days))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of time a product is out of stock when customers want to purchase it.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Cash-to-Cash Cycle Time" datatype="real" name="[Cash-to-Cash Cycle Time]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(days_inventory_outstanding)+SUM(days_sales_outstanding)-SUM(days_payable_outstanding)"/>
      <desc>
         <formatted-text>
            <run>The number of days between paying for raw materials and receiving payment from customers.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Supplier On-Time Delivery" datatype="real" name="[Supplier On-Time Delivery]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(on)-SUM(time_deliveries)/SUM(total_deliveries))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of supplier deliveries that arrive within the agreed time window.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Forecast Accuracy" datatype="real" name="[Forecast Accuracy]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="100-(SUM(abs)(SUM(forecast)-SUM(actual))/SUM(actual))*100"/>
      <desc>
         <formatted-text>
            <run>Measures how closely demand forecasts match actual sales or consumption.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Ore Recovery Rate" datatype="real" name="[Ore Recovery Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(recovered_mineral_weight)/SUM(total_mineral_in_ore))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of valuable mineral recovered from ore during processing.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Tons Per Operating Hour" datatype="real" name="[Tons Per Operating Hour]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_tons_processed)/SUM(total_operating_hours)"/>
      <desc>
         <formatted-text>
            <run>Average tonnage of material extracted or processed per hour of operation.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Haul Truck Cycle Time" datatype="real" name="[Haul Truck Cycle Time]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(average)(SUM(return_to_load_point_time)-SUM(departure_from_load_point_time))"/>
      <desc>
         <formatted-text>
            <run>Average time for a haul truck to complete one full cycle (load, haul, dump, return).</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Project Schedule Performance Index (SPI)" datatype="real" name="[Project Schedule Performance Index (SPI)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(earned_value)/SUM(planned_value)"/>
      <desc>
         <formatted-text>
            <run>Measures project schedule efficiency against planned timeline.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Construction Cost Performance Index (CPI)" datatype="real" name="[Construction Cost Performance Index (CPI)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(earned_value)/SUM(actual_cost)"/>
      <desc>
         <formatted-text>
            <run>Measures project cost efficiency against budget.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Rework Rate" datatype="real" name="[Rework Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(hours_spent_on_rework)/SUM(total_project_hours))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of work that must be redone due to defects or errors.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Crop Yield Per Acre" datatype="real" name="[Crop Yield Per Acre]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_harvest)(SUM(bushels)/SUM(tons))/SUM(total_acres_planted)"/>
      <desc>
         <formatted-text>
            <run>Amount of crop produced per unit of cultivated land area.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Feed Conversion Ratio" datatype="real" name="[Feed Conversion Ratio]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_feed_consumed)(SUM(kg))/SUM(total_weight_gained)(SUM(kg))"/>
      <desc>
         <formatted-text>
            <run>Amount of feed required to produce one unit of livestock product.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Customer Satisfaction Score (CSAT)" datatype="real" name="[Customer Satisfaction Score (CSAT)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_satisfied_customers)/SUM(total_survey_responses))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of customers rating their experience as satisfactory or better.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Net Promoter Score (NPS)" datatype="real" name="[Net Promoter Score (NPS)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(%_promoters)(9-10)-SUM(%_detractors)(0-6)"/>
      <desc>
         <formatted-text>
            <run>Measures customer loyalty by likelihood to recommend to others.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="First Call Resolution Rate" datatype="real" name="[First Call Resolution Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(issues_resolved_on_first_contact)/SUM(total_issues))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of customer issues resolved during the first contact.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Regulatory Compliance Rate" datatype="real" name="[Regulatory Compliance Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(passed_audits)/SUM(total_audits))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of required compliance checks or audits passed without findings.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Audit Findings Per Period" datatype="real" name="[Audit Findings Per Period]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(count_of_findings_requiring_action)"/>
      <desc>
         <formatted-text>
            <run>Number of compliance or quality audit findings requiring corrective action.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Carbon Footprint Per Product" datatype="real" name="[Carbon Footprint Per Product]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_co2_equivalent_emissions)/SUM(total_units_produced)"/>
      <desc>
         <formatted-text>
            <run>Total greenhouse gas emissions attributable to producing one unit of product.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Waste Diversion Rate" datatype="real" name="[Waste Diversion Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(waste_diverted)/SUM(total_waste_generated))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of total waste diverted from landfill through recycling, composting, or reuse.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Renewable Energy Percentage" datatype="real" name="[Renewable Energy Percentage]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(renewable_energy_consumed)/SUM(total_energy_consumed))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of total energy consumption from renewable sources.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Warranty Claim Rate" datatype="real" name="[Warranty Claim Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_warranty_claims)/SUM(total_products_sold))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of products sold that result in warranty claims within coverage period.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Customer Retention Rate" datatype="real" name="[Customer Retention Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="((SUM(customers_at_end)-SUM(new_customers))/SUM(customers_at_start))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of customers who continue doing business over a defined period.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Labor Productivity" datatype="real" name="[Labor Productivity]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_output)(SUM(units)/SUM(revenue))/SUM(total_labor_hours)"/>
      <desc>
         <formatted-text>
            <run>Output produced per labor hour worked.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Schedule Adherence" datatype="real" name="[Schedule Adherence]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(tasks_completed_on_schedule)/SUM(total_planned_tasks))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of planned tasks or shifts completed on schedule.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Equipment Reliability Factor" datatype="real" name="[Equipment Reliability Factor]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(actual_operating_time)/SUM(planned_operating_time))*100"/>
      <desc>
         <formatted-text>
            <run>Ratio of actual productive time to planned production time for equipment.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Quality Defect Rate" datatype="real" name="[Quality Defect Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(number_of_defects)/SUM(total_units_inspected))*100"/>
      <desc>
         <formatted-text>
            <run>Number of defects identified per units produced or inspected.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Process Capability Index (Cpk)" datatype="real" name="[Process Capability Index (Cpk)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(min)((SUM(usl)-SUM(mean))/SUM(3σ,)(SUM(mean)-SUM(lsl))/SUM(3σ))"/>
      <desc>
         <formatted-text>
            <run>Statistical measure of how well a process can produce output within specification limits.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Mean Time To Detect (MTTD)" datatype="real" name="[Mean Time To Detect (MTTD)]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(sum_of_detection_times)/SUM(number_of_incidents)"/>
      <desc>
         <formatted-text>
            <run>Average time between when an incident occurs and when it is detected.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Asset Utilization Rate" datatype="real" name="[Asset Utilization Rate]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="(SUM(productive_time)/SUM(total_available_time))*100"/>
      <desc>
         <formatted-text>
            <run>Percentage of time assets are actively used for productive purposes.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Revenue Per Employee" datatype="real" name="[Revenue Per Employee]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_revenue)/SUM(total_number_of_employees)"/>
      <desc>
         <formatted-text>
            <run>Total revenue generated divided by number of employees.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Backlog-to-Revenue Ratio" datatype="real" name="[Backlog-to-Revenue Ratio]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(total_project_backlog)/SUM(annual_revenue)"/>
      <desc>
         <formatted-text>
            <run>Value of contracted but not yet executed work compared to annual revenue.</run>
         </formatted-text>
      </desc>
   </column>
   <column caption="Pipeline Integrity Index" datatype="real" name="[Pipeline Integrity Index]" role="measure" type="quantitative">
      <calculation class_="tableau" formula="SUM(weighted_average_of)(SUM(inspection_results)+SUM(corrosion_rate)+SUM(leak_incidents))"/>
      <desc>
         <formatted-text>
            <run>Composite score measuring the condition and reliability of pipeline infrastructure.</run>
         </formatted-text>
      </desc>
   </column>
