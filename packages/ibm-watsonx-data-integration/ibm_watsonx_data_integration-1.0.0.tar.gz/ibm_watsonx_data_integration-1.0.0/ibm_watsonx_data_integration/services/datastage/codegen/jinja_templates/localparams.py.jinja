{% for param in parameters %}
{% if param["type"] == "List" %}
{{ composer }}.add_local_parameter(parameter_type = "{{ param["type"] }}", name = "{{ param["name"] }}", valid_values = {{ param["valid_values"] }}, value = {{ param["value"] }}{{ param["extras"] }})
{% else %}
{{ composer }}.add_local_parameter(parameter_type = "{{ param["type"] }}", name = "{{ param["name"] }}", value = {{ param["value"] }}{{ param["extras"] }})
{% endif %}
{% endfor %}
