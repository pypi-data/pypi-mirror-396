<?xml version="1.0"?>
<output>
    <figure nfig="1" split="1">
        <title mesh_name='True'>Snapshots of the solution</title>
        <axis type='color+contour' data_origin="approx" posx="'snaps'" posy="0" aspect="'equal'" snaps="0, 0.5, 1.0">
        </axis>
        <axis type='color+contour' data_origin="reference" posx="'snaps'" posy="1" aspect="'equal'" snaps="0, 0.5, 1.0">
        </axis>
    </figure>
    <figure nfig="2">
        <animation type='color+contour' data_origin="approx">
            <plot type="pcolor" show_title="True" show_ylabel="True" show_colorbar="True"></plot>
        </animation>
    </figure>
    <axis_types>
        <axis name='color+contour'>
            <plot type="pcolor" show_title="True" show_ylabel="True" show_colorbar="True"></plot>
            <plot type="contour" levels="0.1, 0.3" cmap="'Greys'"></plot>
        </axis>
    </axis_types>
</output>
