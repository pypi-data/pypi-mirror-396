<?xml version="1.0"?>
<output>
    <figure nfig="1" split="1">
        <title mesh_name="True">Plot of the exact solution and the approximation</title>
        <axis type="full" data_origin="reference" posx="0" aspect="'equal'">
        </axis>
        <axis data_origin="approx" posx="1" aspect="'equal'">
            <plot type="pcolor"   show_title="True" show_ylabel="False" show_colorbar="True"></plot>
            <plot type="contour" show_title="True" show_ylabel="True" cmap="'Greys'"></plot>
        </axis>
        <axis data_origin="error" posx="2" aspect="'equal'">
            <plot type="pcolor"  show_title="True" show_ylabel="False" show_colorbar="True"></plot>
        </axis>
    </figure>
    <figure nfig="2">
        <title mesh_name='True'>Plot of the mesh</title>
        <axis>
            <plot type="plotmesh" cent='True'></plot>
        </axis>
    </figure>
    <axis_types>
        <axis name="full">
            <plot type="pcolor" show_title="True" show_ylabel="True" show_colorbar="True"></plot>
            <plot type="plotmesh" details="'none'"></plot>
            <plot type="contour"  cmap="'Greys'"></plot>
        </axis>
    </axis_types>
</output>
