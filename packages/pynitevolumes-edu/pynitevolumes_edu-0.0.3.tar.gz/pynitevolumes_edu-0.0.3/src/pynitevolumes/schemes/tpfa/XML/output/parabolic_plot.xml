<?xml version="1.0"?>
<output>
    <figure nfig="1" nline="1" ncol="3" split="1">
        <title mesh_name='True'>Plot of the exact solution and the approximation</title>
        <axis posx="0" aspect="equal">
            <plot type="pcolor" data_origin="reference" title="True" ylabel="True" colorbar="True"></plot>
            <plot type="plotmesh" details="none"></plot>
            <plot type="contour" data_origin="reference" cmap="Greys"></plot>
        </axis>
        <axis posx="1" aspect="equal">
            <plot type="pcolor" data_origin="approx"  title="True" ylabel="False" colorbar="True"></plot>
            <plot type="contour" data_origin="approx" title="True" ylabel="True" cmap="Greys"></plot>
        </axis>
        <axis posx="2" aspect="equal">
            <plot type="pcolor" data_origin="error" title="True" ylabel="False" colorbar="True"></plot>
        </axis>
    </figure>
    <figure nfig="2" split="block">
        <title mesh_name='True'>Plot of the mesh</title>
        <axis>
            <plot type="plotmesh" cent='True'></plot>
        </axis>
    </figure>
    <animation nfig='9'>
        <title mesh='True' dt='True' t='True'>Ã‰volution</title>
        <draw u='approx' type='pcolor_discrete' format='primal' vmin='0.0' vmax='0.5'></draw>
    </animation>
