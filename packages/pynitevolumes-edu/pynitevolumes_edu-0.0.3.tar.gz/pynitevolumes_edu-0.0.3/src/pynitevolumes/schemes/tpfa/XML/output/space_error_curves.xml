<?xml version="1.0"?>
<output>
    <error_analysis>
        <figure nfig="8" nline="1" ncol="2">
            <title>Error norm</title>
            <axis posx="0" posy="0">
                <plot xaxis="step" norms="all" show_labels="True" display="False"> </plot>
            </axis>
            <axis posx="1" posy="0">
                <plot xaxis="dof" norms="all" show_labels="True"> </plot>
            </axis>
        </figure>
        <figure nfig="7" nline="2" ncol="1">
            <title>Plot of the error curve</title>
            <axis posx="0" posy="0">
                <plot xaxis="step" norms="L2" show_labels="True"> </plot>
            </axis>
            <axis posx="0" posy="1">
                <plot xaxis="step" norms="H1" show_labels="True"> </plot>
            </axis>
        </figure>
    </error_analysis>
</output>
