<?xml version="1.0"?>
<output>
    <error_analysis>
        <figure nfig="3">
            <title>Error norm</title>
            <axis posx="0" posy="0">
                <plot xaxis="step" norms="L2" show_labels="True">
                    <parameters type="values">
                        0.1, 0.01
                        <cutoff></cutoff>
                    </parameters>
                </plot>
            </axis>
            <axis posx="1" posy="0">
                <plot xaxis="dt" norms="L2" show_labels="True">
                    <parameters type="last" name="mesh">
                        <cutoff display="True" style=":"></cutoff>
                    </parameters>
                </plot>
            </axis>
        </figure>
        <figure>
            <axis>
                <plot xaxis="dof" norms="all" show_labels="True">
                    <parameters type="percents">
                        0.5, 1
                    </parameters>
                </plot>
            </axis>
        </figure>
    </error_analysis>
</output>
