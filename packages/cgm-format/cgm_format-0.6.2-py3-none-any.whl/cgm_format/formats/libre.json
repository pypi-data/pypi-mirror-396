{
  "fields": [
    {
      "name": "Device",
      "type": "string",
      "description": "Device model name (e.g., 'FreeStyle Libre 3')",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "Serial Number",
      "type": "string",
      "description": "Device serial number/UUID",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "Device Timestamp",
      "type": "string",
      "description": "Timestamp of the event in MM-DD-YYYY HH:MM format",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "Record Type",
      "type": "integer",
      "description": "Type of record (0=Historic, 1=Scan, 2=Strip, 4=Insulin, 5=Food, 6=Other)",
      "constraints": {
        "required": true,
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5,
          6
        ]
      }
    },
    {
      "name": "Historic Glucose mg/dL",
      "type": "number",
      "description": "Automatic CGM reading (recorded every 15 minutes, last 8 hours only)",
      "unit": "mg/dL",
      "constraints": {
        "minimum": 0
      }
    },
    {
      "name": "Scan Glucose mg/dL",
      "type": "number",
      "description": "Manual scan glucose reading",
      "unit": "mg/dL",
      "constraints": {
        "minimum": 0
      }
    },
    {
      "name": "Non-numeric Rapid-Acting Insulin",
      "type": "string",
      "description": "Non-numeric rapid-acting insulin notes",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "Rapid-Acting Insulin (units)",
      "type": "number",
      "description": "Rapid-acting (bolus) insulin dose",
      "unit": "units",
      "constraints": {
        "minimum": 0
      }
    },
    {
      "name": "Non-numeric Food",
      "type": "string",
      "description": "Non-numeric food notes",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "Carbohydrates (grams)",
