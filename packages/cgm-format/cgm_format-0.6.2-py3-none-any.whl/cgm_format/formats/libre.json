      "unit": "grams",
      "constraints": {
        "minimum": 0
      }
    },
    {
      "name": "Carbohydrates (servings)",
      "type": "number",
      "description": "Carbohydrate intake in servings",
      "unit": "servings",
      "constraints": {
        "minimum": 0
      }
    },
    {
      "name": "Non-numeric Long-Acting Insulin",
      "type": "string",
