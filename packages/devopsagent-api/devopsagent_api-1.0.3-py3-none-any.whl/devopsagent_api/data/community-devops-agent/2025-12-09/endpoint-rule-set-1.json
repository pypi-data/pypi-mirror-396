{
  "version": "1.0",
  "parameters": {
    "Region": {
      "type": "String",
      "required": false,
      "default": "us-east-1",
      "documentation": "The AWS region to use for the request"
    },
    "AgentSpaceId": {
      "type": "String",
      "required": false,
      "documentation": "The agent space ID for the request"
    }
  },
  "rules": [
    {
      "conditions": [],
      "endpoint": {
        "url": "https://dp.aidevops.{Region}.api.aws",
        "properties": {
          "authSchemes": [
            {
              "name": "sigv4",
              "signingName": "aidevops",
              "signingRegion": "{Region}"
            }
          ]
        },
        "headers": {}
      },
      "type": "endpoint"
    }
  ]
}
