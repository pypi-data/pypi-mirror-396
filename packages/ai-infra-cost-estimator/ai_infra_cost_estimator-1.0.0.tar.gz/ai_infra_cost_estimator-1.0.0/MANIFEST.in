# Include the package data
include ai_infra_cost_estimator/pricing/*.json

# Include documentation
include README.md
include LICENSE

# Include example configurations
recursive-include examples *.yaml

# Exclude development and build artifacts
exclude .gitignore
exclude .git/*
prune __pycache__
prune *.egg-info
prune build
prune dist
prune .pytest_cache
prune .mypy_cache
