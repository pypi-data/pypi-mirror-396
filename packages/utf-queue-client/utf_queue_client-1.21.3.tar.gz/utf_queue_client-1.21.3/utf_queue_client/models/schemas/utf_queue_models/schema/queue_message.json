            "package_info": {
               "type": "string"
            },
            "artifact_id": {
               "maxLength": 36,
               "type": "string"
            },
            "app_version": {
               "maxLength": 50,
               "type": "string"
            }
         },
         "required": [
            "app_name",
            "session_pk_id",
            "test_duration_sec",
            "test_result",
            "test_result_type"
         ]
      },
