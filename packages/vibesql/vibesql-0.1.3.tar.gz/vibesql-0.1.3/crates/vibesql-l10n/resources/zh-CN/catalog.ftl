# VibeSQL 目录错误消息 - 简体中文
# 此文件包含 vibesql-catalog crate 的所有错误消息。

# =============================================================================
# 表错误
# =============================================================================

catalog-table-already-exists = 表 '{ $name }' 已存在
catalog-table-not-found = 未找到表 '{ $table_name }'

# =============================================================================
# 列错误
# =============================================================================

catalog-column-already-exists = 列 '{ $name }' 已存在
catalog-column-not-found = 在表 '{ $table_name }' 中未找到列 '{ $column_name }'

# =============================================================================
# 模式错误
# =============================================================================

catalog-schema-already-exists = 模式 '{ $name }' 已存在
catalog-schema-not-found = 未找到模式 '{ $name }'
catalog-schema-not-empty = 模式 '{ $name }' 不为空

# =============================================================================
# 角色错误
# =============================================================================

catalog-role-already-exists = 角色 '{ $name }' 已存在
catalog-role-not-found = 未找到角色 '{ $name }'

# =============================================================================
# 域错误
# =============================================================================

catalog-domain-already-exists = 域 '{ $name }' 已存在
catalog-domain-not-found = 未找到域 '{ $name }'
catalog-domain-in-use = 域 '{ $domain_name }' 仍被 { $count } 个列使用：{ $columns }

# =============================================================================
# 序列错误
# =============================================================================

catalog-sequence-already-exists = 序列 '{ $name }' 已存在
catalog-sequence-not-found = 未找到序列 '{ $name }'
catalog-sequence-in-use = 序列 '{ $sequence_name }' 仍被 { $count } 个列使用：{ $columns }

# =============================================================================
# 类型错误
# =============================================================================

catalog-type-already-exists = 类型 '{ $name }' 已存在
catalog-type-not-found = 未找到类型 '{ $name }'
catalog-type-in-use = 类型 '{ $name }' 仍被一个或多个表使用

# =============================================================================
# 排序规则和字符集错误
# =============================================================================

catalog-collation-already-exists = 排序规则 '{ $name }' 已存在
catalog-collation-not-found = 未找到排序规则 '{ $name }'
catalog-character-set-already-exists = 字符集 '{ $name }' 已存在
catalog-character-set-not-found = 未找到字符集 '{ $name }'
catalog-translation-already-exists = 翻译 '{ $name }' 已存在
catalog-translation-not-found = 未找到翻译 '{ $name }'

# =============================================================================
# 视图错误
# =============================================================================

catalog-view-already-exists = 视图 '{ $name }' 已存在
catalog-view-not-found = 未找到视图 '{ $name }'
catalog-view-in-use = 视图或表 '{ $view_name }' 仍被 { $count } 个视图使用：{ $views }

# =============================================================================
# 触发器错误
# =============================================================================

catalog-trigger-already-exists = 触发器 '{ $name }' 已存在
catalog-trigger-not-found = 未找到触发器 '{ $name }'

# =============================================================================
# 断言错误
# =============================================================================

catalog-assertion-already-exists = 断言 '{ $name }' 已存在
catalog-assertion-not-found = 未找到断言 '{ $name }'

# =============================================================================
# 函数和过程错误
# =============================================================================

catalog-function-already-exists = 函数 '{ $name }' 已存在
catalog-function-not-found = 未找到函数 '{ $name }'
catalog-procedure-already-exists = 过程 '{ $name }' 已存在
catalog-procedure-not-found = 未找到过程 '{ $name }'

# =============================================================================
# 约束错误
# =============================================================================

catalog-constraint-already-exists = 约束 '{ $name }' 已存在
catalog-constraint-not-found = 未找到约束 '{ $name }'

# =============================================================================
# 索引错误
# =============================================================================

catalog-index-already-exists = 表 '{ $table_name }' 上的索引 '{ $index_name }' 已存在
catalog-index-not-found = 在表 '{ $table_name }' 上未找到索引 '{ $index_name }'

# =============================================================================
# 外键错误
# =============================================================================

catalog-circular-foreign-key = 检测到表 '{ $table_name }' 的循环外键依赖：{ $message }
