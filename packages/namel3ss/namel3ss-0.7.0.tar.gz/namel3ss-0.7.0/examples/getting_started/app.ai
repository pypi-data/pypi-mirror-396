app "getting-started":
  entry_page "home"

page "home" at "/":
  section "hero":
    heading "Getting Started with Namel3ss"
    text "This page uses the modern English-style DSL."

model "default":
  provider "dummy"

ai "greet":
  model "default"
  input from "user_input"

flow "pipeline":
  step "call":
    kind "ai"
    target "greet"
